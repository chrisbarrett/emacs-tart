;; ============================================================================
;; Variables
;; ============================================================================

;; Input ring for history
(defvar comint-input-ring ring)

;; Size of input ring
(defvar comint-input-ring-size int)

;; Regexp matching prompt
(defvar comint-prompt-regexp string)

;; Whether prompt is read-only
(defvar comint-prompt-read-only any)

;; Whether to highlight input
(defvar comint-highlight-input any)

;; Face for prompt
(defvar comint-highlight-prompt symbol)

;; File for saving history
(defvar comint-input-ring-file-name (string | nil))

;; Ignoredups in history
(defvar comint-input-ignoredups any)

;; Whether to scroll for output
(defvar comint-scroll-to-bottom-on-input (symbol | nil))

;; Whether to scroll for output
(defvar comint-scroll-to-bottom-on-output (symbol | nil))

;; Whether to scroll for output
(defvar comint-move-point-for-output (symbol | nil))

;; Show maximum output
(defvar comint-scroll-show-maximum-output any)

;; Max size before truncation
(defvar comint-buffer-maximum-size int)

;; Whether to use prompt regexp
(defvar comint-use-prompt-regexp any)

;; Input filter functions
(defvar comint-input-filter-functions (list ((string) -> any)))

;; Output filter functions
(defvar comint-output-filter-functions (list ((string) -> any)))

;; Preoutput filter functions
(defvar comint-preoutput-filter-functions (list ((string) -> string)))

;; Mode hook
(defvar comint-mode-hook (list ((nil) -> any)))

;; Process echoes
(defvar comint-process-echoes any)

;; Input sender
(defvar comint-input-sender (((process string) -> nil) | nil))

;; Get old input function
(defvar comint-get-old-input (((nil) -> string) | nil))
