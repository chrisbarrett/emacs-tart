;; General command interpreter mode for inferior processes.

;; ============================================================================
;; Mode
;; ============================================================================

;; (comint-mode) - major mode for inferior process
(defun comint-mode () -> nil)

;; ============================================================================
;; Process Creation
;; ============================================================================

;; (make-comint NAME PROGRAM &optional STARTFILE &rest SWITCHES)
;; Create buffer with subprocess running PROGRAM
(defun make-comint (string string &optional string &rest string) -> buffer)

;; (make-comint-in-buffer NAME BUFFER PROGRAM &optional STARTFILE &rest SWITCHES)
(defun make-comint-in-buffer (string buffer string &optional string &rest string) -> buffer)

;; (comint-run PROGRAM &optional SWITCHES) - run program in comint buffer
(defun comint-run (string &optional (list string)) -> nil)

;; (comint-exec BUFFER NAME PROGRAM STARTFILE SWITCHES)
(defun comint-exec (buffer string string (string | nil) (list string)) -> buffer)

;; ============================================================================
;; Input/Output
;; ============================================================================

;; (comint-send-input &optional NO-NEWLINE ARTIFICIAL) - send input to process
(defun comint-send-input (&optional any any) -> nil)

;; (comint-send-string PROCESS STRING) - send string to process
(defun comint-send-string (process string) -> nil)

;; (comint-send-region PROCESS START END) - send region to process
(defun comint-send-region (process (int | marker) (int | marker)) -> nil)

;; (comint-send-eof) - send EOF to process
(defun comint-send-eof () -> nil)

;; (comint-send-invisible PROMPT &optional PROMPT-STRING) - read and send invisibly
(defun comint-send-invisible (string &optional string) -> nil)

