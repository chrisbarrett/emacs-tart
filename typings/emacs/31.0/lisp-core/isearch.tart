(defun isearch-highlight-lines-matching-regexp () -> nil)

;; -----------------------------------------------------------------------------
;; Utility functions
;; -----------------------------------------------------------------------------

;; (isearch-message &optional C-Q-HACK ELLIPSIS)
;; Generate and print the message string.
(defun isearch-message (&optional any any) -> nil)

;; (isearch-message-prefix &optional C-Q-HACK ELLIPSIS NONINCREMENTAL)
