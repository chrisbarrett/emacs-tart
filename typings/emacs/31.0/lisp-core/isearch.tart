;; Type signatures for Emacs incremental search from isearch.el
;;
;; Source: emacs/lisp/isearch.el
;; Emacs version: 31.0
;;
;; Contains incremental search functions and variables.

;; -----------------------------------------------------------------------------
;; Core entry points
;; -----------------------------------------------------------------------------

;; (isearch-forward &optional REGEXP-P NO-RECURSIVE-EDIT)
;; Start incremental search forward.
;; With prefix arg, do a regular expression search instead.
(defun isearch-forward (&optional any any) -> nil)

;; (isearch-backward &optional REGEXP-P NO-RECURSIVE-EDIT)
;; Start incremental search backward.
(defun isearch-backward (&optional any any) -> nil)

;; (isearch-forward-regexp &optional NOT-REGEXP NO-RECURSIVE-EDIT)
;; Start incremental forward search for a regular expression.
(defun isearch-forward-regexp (&optional any any) -> nil)

;; (isearch-backward-regexp &optional NOT-REGEXP NO-RECURSIVE-EDIT)
;; Start incremental backward search for a regular expression.
(defun isearch-backward-regexp (&optional any any) -> nil)

;; (isearch-forward-word &optional NOT-WORD NO-RECURSIVE-EDIT)
;; Start incremental search forward for a word sequence.
(defun isearch-forward-word (&optional any any) -> nil)

;; (isearch-forward-symbol &optional _NOT-SYMBOL NO-RECURSIVE-EDIT)
;; Start incremental search forward for a symbol.
(defun isearch-forward-symbol (&optional any any) -> nil)

;; (isearch-forward-symbol-at-point &optional ARG)
;; Start incremental search for symbol at point.
(defun isearch-forward-symbol-at-point (&optional int) -> nil)

;; (isearch-forward-thing-at-point)
;; Start incremental search for thing at point.
(defun isearch-forward-thing-at-point () -> nil)

;; -----------------------------------------------------------------------------
;; Control functions
;; -----------------------------------------------------------------------------

;; (isearch-mode FORWARD &optional REGEXP OP-FUN RECURSIVE-EDIT REGEXP-FUNCTION)
;; Start Isearch minor mode.
;; Called by isearch-forward, isearch-backward, etc.
(defun isearch-mode (bool &optional any any any any) -> nil)

;; (isearch-done &optional NOPUSH EDIT)
;; Exit Isearch mode.
;; NOPUSH means don't push the string onto the search ring.
(defun isearch-done (&optional any any) -> nil)

;; (isearch-exit)
;; Exit search normally.
;; If first time, just exit; if empty string, switch to nonincremental search.
(defun isearch-exit () -> nil)

;; (isearch-abort)
;; Abort incremental search.
;; If search is successful, go back to starting point.
(defun isearch-abort () -> nil)

;; (isearch-cancel)
;; Terminate search and go back to starting point.
(defun isearch-cancel () -> nil)

;; -----------------------------------------------------------------------------
;; Search iteration
;; -----------------------------------------------------------------------------

;; (isearch-repeat DIRECTION &optional COUNT)
;; Repeat the search in DIRECTION.
(defun isearch-repeat (symbol &optional int) -> nil)

;; (isearch-repeat-forward &optional COUNT)
;; Repeat search forward.
(defun isearch-repeat-forward (&optional int) -> nil)

;; (isearch-repeat-backward &optional COUNT)
;; Repeat search backward.
(defun isearch-repeat-backward (&optional int) -> nil)

;; -----------------------------------------------------------------------------
;; Search state management
;; -----------------------------------------------------------------------------

;; (isearch-update)
;; Called after each command to update display.
(defun isearch-update () -> nil)

;; (isearch-edit-string)
;; Edit the search string in the minibuffer.
(defun isearch-edit-string () -> nil)

;; (isearch-push-state)
;; Push the current search state onto the stack.
(defun isearch-push-state () -> nil)

;; (isearch-pop-state)
;; Pop the top state from the stack.
(defun isearch-pop-state () -> nil)

;; (isearch-clean-overlays)
;; Clean up lazy highlight overlays.
(defun isearch-clean-overlays () -> nil)

;; -----------------------------------------------------------------------------
;; Yank operations
;; -----------------------------------------------------------------------------

;; (isearch-yank-word-or-char)
;; Yank next word or character from buffer into search string.
(defun isearch-yank-word-or-char () -> nil)

;; (isearch-yank-symbol-or-char)
;; Yank next symbol or character from buffer into search string.
(defun isearch-yank-symbol-or-char () -> nil)

;; (isearch-yank-char)
;; Yank char from buffer onto end of search string.
(defun isearch-yank-char () -> nil)

;; (isearch-yank-line)
;; Yank rest of line from buffer onto end of search string.
(defun isearch-yank-line () -> nil)

;; (isearch-yank-kill)
;; Yank last kill onto end of search string.
(defun isearch-yank-kill () -> nil)

;; (isearch-yank-until-char CHAR)
;; Yank from point to before CHAR into search string.
(defun isearch-yank-until-char (int) -> nil)

;; (isearch-yank-pop)
;; Replace current search string with next kill ring item.
(defun isearch-yank-pop () -> nil)

;; (isearch-yank-x-selection)
;; Yank X selection onto end of search string.
(defun isearch-yank-x-selection () -> nil)

;; -----------------------------------------------------------------------------
;; Delete/edit operations
;; -----------------------------------------------------------------------------

;; (isearch-delete-char)
;; Undo last input item during search.
(defun isearch-delete-char () -> nil)

;; (isearch-del-char)
;; Delete char from end of search string.
(defun isearch-del-char () -> nil)

;; (isearch-printing-char &optional CHAR COUNT)
;; Append a printing character to the search string.
(defun isearch-printing-char (&optional int int) -> nil)

;; -----------------------------------------------------------------------------
;; History ring
;; -----------------------------------------------------------------------------

;; (isearch-update-ring STRING &optional REGEXP)
;; Add STRING to the appropriate search ring.
(defun isearch-update-ring (string &optional any) -> nil)

;; (isearch-ring-advance)
;; Advance to the next search string from the history ring.
(defun isearch-ring-advance () -> nil)

;; (isearch-ring-retreat)
;; Go back to the previous search string from the history ring.
(defun isearch-ring-retreat () -> nil)

;; (isearch-ring-advance-edit)
;; Advance in minibuffer edit mode.
(defun isearch-ring-advance-edit () -> nil)

;; (isearch-ring-retreat-edit)
;; Retreat in minibuffer edit mode.
(defun isearch-ring-retreat-edit () -> nil)

;; (isearch-complete)
;; Complete the search string from the search rings.
(defun isearch-complete () -> nil)

;; (isearch-complete-edit)
;; Complete the search string in minibuffer edit mode.
(defun isearch-complete-edit () -> nil)

