;; Internal/Utility
;; ============================================================================

;; (windmove-find-other-window DIR &optional ARG WINDOW) - find window in direction
(defun windmove-find-other-window (symbol &optional int window) -> (window | nil))

;; (windmove-do-window-select DIR &optional ARG WINDOW) - select window in direction
(defun windmove-do-window-select (symbol &optional int window) -> nil)

