;; ============================================================================
;; Variables
;; ============================================================================

;; Directory name or wildcard spec for current buffer
(defvar dired-directory (string | (list string)))

;; Switches passed to ls
(defvar dired-listing-switches string)

;; Current mark character
(defvar dired-marker-char int)

;; Character flagging files for deletion
(defvar dired-del-marker int)

;; Actual switches used for current listing
(defvar dired-actual-switches (string | nil))

;; Alist of listed directories and positions
(defvar dired-subdir-alist (list (pair string int)))

;; Mode hook
(defvar dired-mode-hook (list ((nil) -> any)))

;; Whether to confirm file operations
(defvar dired-no-confirm (list symbol))

;; Whether to auto-revert
(defvar dired-auto-revert-buffer any)

;; Hide details
(defvar dired-hide-details-mode any)

;; How to handle recursive delete
(defvar dired-recursive-deletes (symbol | nil))

;; How to handle recursive copy
(defvar dired-recursive-copies (symbol | nil))

;; Deletion confirmation
(defvar dired-deletion-confirmer ((string) -> any))

;; Omit files mode
(defvar dired-omit-mode any)

;; Free disk space display
(defvar dired-free-space (symbol | nil))

;; Keep file modification times on copy
(defvar dired-copy-preserve-time any)

;; Create destination dir if missing
(defvar dired-create-destination-dirs (symbol | nil))
