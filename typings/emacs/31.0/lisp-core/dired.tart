;; Type signatures for Emacs dired.el
;;
;; Source: emacs/lisp/dired.el
;; Emacs version: 31.0
;;
;; Dired - the directory editor.

;; ============================================================================
;; Entry Points
;; ============================================================================

;; (dired DIRNAME &optional SWITCHES) - open directory in Dired mode
(defun dired (string &optional string) -> buffer)

;; (dired-other-window DIRNAME &optional SWITCHES) - open in other window
(defun dired-other-window (string &optional string) -> buffer)

;; (dired-other-frame DIRNAME &optional SWITCHES) - open in new frame
