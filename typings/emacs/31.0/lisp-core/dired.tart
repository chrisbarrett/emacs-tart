(defun dired-other-tab (string &optional string) -> buffer)

;; (dired-noselect DIR-OR-LIST &optional SWITCHES) - return buffer without selecting
(defun dired-noselect ((string | (list string)) &optional string) -> buffer)

;; (dired-jump &optional OTHER-WINDOW FILE-NAME) - jump to current file in Dired
(defun dired-jump (&optional any string) -> nil)

;; (dired-jump-other-window &optional FILE-NAME) - jump in other window
(defun dired-jump-other-window (&optional string) -> nil)

