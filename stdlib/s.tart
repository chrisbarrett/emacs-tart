;; Word case transformations
(defun s-split-words (string) -> (list string))
(defun s-lower-camel-case (string) -> string)
(defun s-upper-camel-case (string) -> string)
(defun s-snake-case (string) -> string)
(defun s-dashed-words (string) -> string)
(defun s-capitalized-words (string) -> string)
(defun s-titleized-words (string) -> string)
(defun s-word-initials (string) -> string)

;; Other utilities
(defun s-index-of (string string &optional truthy) -> (int | nil))
(defun s-presence (string) -> (string | nil))
(defun s-format (string ((string) -> string) &optional any) -> string)
(defun s-count-matches (string string &optional int int) -> int)
(defun s-wrap (string string &optional string) -> string)
