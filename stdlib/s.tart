;;; s.tart --- Type signatures for s.el string manipulation library

;; Whitespace trimming
(defun s-trim (string) -> string)
(defun s-trim-left (string) -> string)
(defun s-trim-right (string) -> string)
(defun s-chomp (string) -> string)
(defun s-collapse-whitespace (string) -> string)

;; Padding and wrapping
(defun s-word-wrap (int string) -> string)
(defun s-center (int string) -> string)
(defun s-pad-left (int string string) -> string)
(defun s-pad-right (int string string) -> string)

;; Truncation and extraction
(defun s-truncate (int string) -> string)
(defun s-left (int string) -> string)
(defun s-right (int string) -> string)
(defun s-chop-left (int string) -> string)
(defun s-chop-right (int string) -> string)

;; Prefix/suffix operations
(defun s-chop-prefix (string string) -> string)
